<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Youtube playlist saver</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" 
            rel="stylesheet" 
            integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" 
            crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col 2">
                <h1>Youtube playlist saver</h1>
            </div>
        </div>
        <div class="row">
            <% playlistTitles.forEach(function(title) { %>
                <p><%= title %></p>
            <% }); %>
            <p>Playlists gathered: <%= itemsCount %></p>
        </div>
    </div>
</body>
</html>